<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nayidisha.twitter.domain.TAOAuthToken" table="ta_access_token">
		<id name="id" column="id" />
		<property name="screenName"  column="screen_name"/>
		<property name="token" column="token"/>
		<property name="tokenSecret" column="token_secret"/>		
		
		<property name="createdDate" type="timestamp" column="created_date"/>
		<property name="updatedDate" type="timestamp" column="updated_date"/>
		<property name="createdBy" type="string" column="created_by"/>
		<property name="updatedBy" type="string" column="updated_by"/>			
	</class>
</hibernate-mapping>